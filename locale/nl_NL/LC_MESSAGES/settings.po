msgstr[0] "Het voorvoegsel voor deze server is {prefixes}"
msgstr[1] "De voorvoegsels voor deze server zijn {prefixes}"

#: cogs/settings.py:23 cogs/settings.py:30 cogs/settings.py:45
#: cogs/settings.py:88
msgid "and"
msgstr "en"

#: cogs/settings.py:26
msgid "Please specify which prefix(es) should be added."
msgstr "Geef aan welke voorvoegsel(s) moet(en) worden toegevoegd."

#: cogs/settings.py:29
#, python-brace-format
msgid "{prefixes} is already used"
msgid_plural "{prefixes} are already used"
msgstr[0] "{prefixes} wordt al gebruikt"
msgstr[1] "{prefixes} worden al gebruikt"

#: cogs/settings.py:38
#, python-brace-format
msgid "Added {n} prefix."
msgid_plural "Added {n} prefixes."
msgstr[0] "{n} voorvoegsel toegevoegd."
msgstr[1] "{n} voorvoegsels toegevoegd."

#: cogs/settings.py:41
msgid "Please specify which prefix(es) should be deleted."
msgstr "Geef aan welke voorvoegsel(s) moet(en) worden verwijderd."

#: cogs/settings.py:44
#, python-brace-format
msgid "{prefixes} isn't used"
msgid_plural "{prefixes} aren't used"
msgstr[0] "{prefixes} wordt niet gebruikt"
msgstr[1] "{prefixes} worden niet gebruikt"

#: cogs/settings.py:53
#, python-brace-format
msgid "Removed {n} prefix."
msgid_plural "Removed {n} prefixes."
msgstr[0] "{n} voorvoegsel verwijderd."
msgstr[1] "{n} voorvoegsels verwijderd."

#: cogs/settings.py:57
#, python-brace-format
msgid "Server prefix reset to `{default}`."
msgstr "Server voorvoegsel hersteld naar `{default}`."

#: cogs/settings.py:65
#, python-brace-format
msgid "The breaker character for this server is `{breaker}`"
msgstr "Het breker teken voor deze server is `{breaker}`"

#: cogs/settings.py:68
msgid "Please use `set <breaker_character>`"
msgstr "Gebruik AUB `set <breker_teken>`"

